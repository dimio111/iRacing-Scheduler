<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.dimi.iracing.scheduler.controllers.LabelViewController">
    <children>
        <HBox maxWidth="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="300.0">
            <children>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Laps:">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label fx:id="laps"/>
                    </children>
                </HBox>
            </children>
        </HBox>
        <HBox maxWidth="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="300.0">
            <children>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label prefHeight="26.0" prefWidth="47.0" text="Track:">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label fx:id="track">
                            <font>
                                <Font size="12.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
            </children>
        </HBox>
        <HBox maxWidth="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="300.0">
            <children>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Road type">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label fx:id="roadType"/>
                    </children>
                </HBox>
            </children>
        </HBox>
        <HBox maxWidth="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="300.0">
            <children>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Alarm set:">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label fx:id="alarmSet"/>
                    </children>
                </HBox>
            </children>
        </HBox>
        <HBox maxWidth="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="300.0">
            <children>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Time until race:">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label fx:id="timeUntilRace"/>
                    </children>
                </HBox>
            </children>
        </HBox>
    </children>
</VBox>