<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>

<?import javafx.scene.layout.VBox?>
<VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="be.dimi.iracing.scheduler.controllers.TableViewController">
    <children>
        <TableView fx:id="raceTableView" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="300.0"
                   tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="dateColumn" editable="false" maxWidth="90.0" minWidth="75.0" prefWidth="75.0"
                             sortable="false" text="DATE"/>
                <TableColumn fx:id="hourColumn" editable="false" maxWidth="90.0" minWidth="70.0" prefWidth="70.0"
                             sortable="false" text="HOUR"/>
                <TableColumn fx:id="raceColumn" editable="false" minWidth="225.0" prefWidth="300.0" sortable="false"
                             text="RACE"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</VBox>